{
  "name": "Community Boating Flag Color",
  "version": "1.10",
  "manifest_version": 2,
  "description": "Indicates what color flag Boston's Community Boating is currently flying.",
  "browser_action": {
    "default_title": "Community Boating",
    "default_icon": "images/error.png"
  },
  "icons": {
    "16":  "images/icon16.png",
    "48":  "images/icon48.png", 
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": [ "background.js" ]
  },
  "web_accessible_resources": [
    "images/green.png",
    "images/yellow.png",
    "images/red.png",
    "images/closed.png",
    "images/error.png"
  ],
  "permissions": [
    "notifications",
    "http://www.community-boating.org/",
    "https://portal2.community-boating.org/"
  ]
}
